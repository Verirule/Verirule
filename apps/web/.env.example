# Supabase (client-side)
NEXT_PUBLIC_SUPABASE_URL=
# Preferred key type (new):
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY=
# Backwards compatible (legacy):
NEXT_PUBLIC_SUPABASE_ANON_KEY=
# Comma-separated OAuth providers enabled in Supabase Auth (for example: google,github)
NEXT_PUBLIC_SUPABASE_OAUTH_PROVIDERS=google,github
# Public site URL used for OAuth/callback redirects in production
# Example: https://www.verirule.xyz
NEXT_PUBLIC_SITE_URL=

# Verirule API
VERIRULE_API_URL=

# Stripe
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=
STRIPE_PRICE_PRO=
STRIPE_PRICE_BUSINESS=

# Debug pages (server-side gate)
VERIRULE_ENABLE_DEBUG_PAGES=false
